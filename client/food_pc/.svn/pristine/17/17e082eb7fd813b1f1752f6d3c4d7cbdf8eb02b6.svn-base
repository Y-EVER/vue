// 表格
.el-table td, .el-table th{padding: 8px 0;text-align: center;}
.el-tabs--border-card>.el-tabs__content{padding: 0 !important;}
.el-table--border th{border-right: none;background: #f4f8fd;color: #505658;}
.el-table__body-wrapper .el-table__row:nth-child(n){background: #FBFBFB;}
.el-table__body-wrapper .el-table__row:nth-child(2n){background: #fff;}
.el-table td{padding: 5px 0;}
//表单左右结构型
.form_one .el-form-item{border: 1px solid #DCDFE6;border-bottom: none;margin-bottom: 0;}
.form_one >div:last-child .el-form-item{border-bottom: 1px solid #DCDFE6;}
.form_one .el-form-item__label{width: 24%;padding: 15px;border-right: 1px solid #DCDFE6;margin-right: 15px;background: #f4f7fc;}
.form_one .el-form-item__content{width: 72%;}
.form_one > div > .el-form-item:nth-child(2n-1){border-right: none;}
.el-input__inner{border-radius: 2px;}
.dialog-footer{display: flex;justify-content: flex-end;margin-top: 20px;}
//表单单行
.form_two .el-form-item{border: 1px solid #DCDFE6;border-bottom: none;}
.form_two .el-form-item:last-child{border-top: 1px solid #DCDFE6 !important;padding-top: 20px;}
.form_two .el-form-item__label{padding: 15px;width: 12% !important;border-right: 1px solid #DCDFE6;margin-right: 15px;background: #f4f7fc;}
.form_two .el-form-item__content{width: 80%;margin-left: 13% !important;}
.form_two .el-input{margin-top: 15px;}
.form_two .el-select{width: 100%;}
.form_two .line{text-align: center;margin-top: 15px;}
.form_two .el-textarea{padding-top: 15px;padding-bottom: 15px;}
.form_two .el-textarea__inner{height: 100px;}
.form_two .line_h_pad .el-form-item__label{padding: 45px 15px !important;}
.form_two .el-form-item:last-child{border: none;}
.form_two .el-form-item:last-child .el-form-item__content{width: 100% !important;margin-left: 0 !important;}
.form_two .el-form-item:last-child .el-form-item__content button{float: right;margin-left: 20px;}
.form_two .el-form-item{margin-bottom: 0;}
.form_two .el-upload--picture-card{width: 100px;height: 100px;line-height: 100px;margin: 15px 0;}

